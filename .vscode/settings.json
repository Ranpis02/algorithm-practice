{
  "files.exclude": {
    "**/.temp": true,
    "**/*.exe": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/bin": true
  },
  "files.watcherExclude": {
    "**/.temp/**": true,
    "**/bin/**": true
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "code-runner.executorMap": {
    "cpp": "cd $dir && if (!(Test-Path '.temp')) { New-Item -ItemType Directory -Name '.temp' | Out-Null }; g++ -g '$fileName' -o '.temp/$fileNameWithoutExt.exe' && & '.temp/$fileNameWithoutExt.exe'",
    "java": "cd $dir; if (!(Test-Path '$workspaceRoot\\bin')) { New-Item -ItemType Directory -Path '$workspaceRoot\\bin' | Out-Null }; javac -d '$workspaceRoot\\bin' '$fullFileName'; if ($?) { java -cp '$workspaceRoot\\bin' '$fileNameWithoutExt' }"
  },
  "java.project.outputPath": "bin",
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.preserveFocus": false,
  "C_Cpp.default.compilerPath": "D:\\Environment\\Scoop\\apps\\mingw\\current\\bin\\g++.exe",
  "files.associations": {
    "*.cjson": "jsonc",
    "*.wxss": "css",
